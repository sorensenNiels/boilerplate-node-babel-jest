{
  "name": "babel",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --forceExit",
    "build": "npm run clean && babel src --out-dir dist",
    "start": "npm run build && node dist/server.js",
    "watch": "nodemon --exec babel-node src/server.js",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint .",
    "docs": "documentation readme src --section=API",
    "postdocs": "git add README.md",
    "clean": "rimraf dist",
    "xprebuild": "npm run clean",
    "xpreversion": "npm run lint && npm test && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "xprepublish": "npm run build",
    "xpostpublish": "git push origin master --follow-tags"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "src"
  ],
  "xhusky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^5.2.0",
    "documentation": "^9.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest-cli": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.11",
    "opn-cli": "^4.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "standard-changelog": "^2.0.7"
  },
  "dependencies": {
    "async": "^3.0.1-0",
    "axios": "^0.18.0",
    "config": "^3.1.0",
    "express": "^4.16.4",
    "fastify": "^2.2.0",
    "lodash": "^4.17.11",
    "pino": "^5.12.2"
  }
}
